# Makefile for the AW_Lib
#
# @author Marcelo Cora√ßa de Freitas <marcelo.batera@gmail.com> 


libs:
	gnatmake -P awlib.gpr
tests: libs
	gnatmake -P awlib-tests.gpr

all: libs



clean-libs:
	gnatclean -P awlib.gpr
clean-tests:
	gnatclean -P awlib-tests.gpr
clean: clean-tests clean-libs
	@echo "All clean"



docs:
	@-./gendoc.sh
	@echo "The documentation is generated by a bash script. Then it might fail in other platforms"
